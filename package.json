{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier -w .",
    "lint-all": "eslint 'src/**/*.ts' --fix",
    "lint": "lint-staged",
    "prebuild": "rimraf dist",
    "setup": "husky install",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "start:staging": "NODE_ENV=staging node dist/main",
    "start": "node dist/main",
    "install": "nest build",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest -c jest.e2e.js",
    "test:watch": "NODE_ENV=test jest --watch",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.2.3",
    "@nestjs/graphql": "^10.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.2.3",
    "apollo-server": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-flow": "^3.2.0",
    "graphql": "^16.3.0",
    "graphql-query-complexity": "^0.11.0",
    "graphql-subscriptions": "^2.0.0",
    "handlebars": "^4.7.7",
    "mongoose": "^6.2.8",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.2.3",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "apollo-server-core": "^3.6.7",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mongodb-memory-server": "^8.4.2",
    "mongoose-encryption": "^2.1.2",
    "nodemailer-mock": "^1.5.11",
    "prettier": "^2.5.1",
    "smtp-tester": "^1.2.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.8",
    "ts-morph": "^13.0.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  }
}
